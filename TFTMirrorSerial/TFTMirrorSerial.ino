void setup() {
  Serial.begin(74400 );
  while (!Serial) {
    ; // wait for serial port to connect. Needed for native USB port only
  }
  tft.begin();
  tft.setFont(&FreeSans9pt7b);
  tft.setRotation(1);
  tft.setTextWrap(true);
  tft.fillScreen(ILI9341_BLACK);
  tft.drawBitmap(0, 0, myBitmap, 320, 240, ILI9341_GREEN);
  delay(3000);
  tft.fillScreen(ILI9341_BLACK);
}


void loop(void) {

  while (Serial.available() > 0) {
   espMessage += Serial.readString();
  }
  if(espMessage != ""){
    Serial.flush();
    if(oldMessage != "")clearText(oldMessage);
     setScreenText(espMessage);
     oldMessage = espMessage;
     espMessage = "";
     Serial.flush();
  }
}

